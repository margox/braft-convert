{
  "name": "braft-convert",
  "version": "2.3.0",
  "description": "A convert helper for Braft Editor.",
  "main": "dist/index.js",
  "scripts": {
    "build": "rm -rf dist/ && BABEL_ENV=production ./node_modules/.bin/babel src --copy-files --source-maps --extensions .es6,.es,.js,.jsx --out-dir dist",
    "prepare": "npm run build",
    "start": "webpack-dev-server -d --history-api-fallback --hot --inline --progress --colors --port 5997"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/margox/braft-convert.git"
  },
  "keywords": [
    "convert",
    "braft"
  ],
  "author": "Margox",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/margox/braft-convert/issues"
  },
  "homepage": "https://github.com/margox/braft-convert#readme",
  "dependencies": {
    "draft-convert": "^2.0.0",
    "draft-js": "^0.10.3"
  },
  "devDependencies": {
    "babel-cli": "^6.16.0",
    "babel-core": "^6.17.0",
    "babel-loader": "^6.2.5",
    "babel-preset-es2015": "^6.5.0",
    "babel-preset-react": "^6.5.0",
    "babel-preset-stage-0": "^6.24.1",
    "babel-runtime": "^6.5.0",
    "html-webpack-plugin": "^2.17.0",
    "react": "^16.0.0",
    "react-dom": "^16.0.0",
    "react-hot-loader": "next",
    "webpack": "^2.6.1",
    "webpack-dev-server": "^2.4.5",
    "webpack-hot-middleware": "^2.18.0",
    "webpack-merge": "^4.1.0"
  },
  "peerDependencies": {
    "react": "^16.0.0 || ^17.0.1"
  }
}
